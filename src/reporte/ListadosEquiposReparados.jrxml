	<queryString language="SQL">
		<![CDATA[SELECT count(soporte.oid) AS count,soporte.oid
,soporte.fecharec,soporte.fechasal
,soporte.desequipo,soporte.dessoporte
,cliente.cedula AS cliId,cliente.nombre AS cliName
,analista.cedula AS anaId ,analista.nombre AS anaName FROM soporte INNER JOIN  inventario
ON soporte.codbien=inventario.codbien
INNER JOIN analista
ON soporte.analista=analista.codigo
INNER JOIN cliente
ON soporte.cliente=cliente.codigo
