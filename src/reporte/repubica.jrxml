	<parameter name="Dfecha" class="java.util.Date" isForPrompting="false"/>
	<parameter name="Plogo" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT
     correpo.`codigo`,
     correpo.`nro_oficio`,
     correpo.`fecha_rec`,
     correpo.`cod_doc`,
     correpo.`fecha_des`,
     correpo.`cod_des`,
     documento.`den_doc`,
     remite.`den_rem`,
     destina.`den_des`,
     correpo.`grupo`
FROM
     `documento` documento INNER JOIN `correpo` correpo ON documento.`cod_doc` = correpo.`cod_doc`
     INNER JOIN `destina` destina ON correpo.`cod_des` = destina.`cod_des`
     INNER JOIN `remite` remite ON correpo.`cod_rem` = remite.`cod_rem`
WHERE
     correpo.edo_reg = "A"
 AND correpo.cod_des = $P{Vcoddes}
 AND correpo.cod_doc = $P{Vcoddoc}
 AND correpo.grupo   = $P{Vgrupo}
 AND correpo.fecha_des = $P{Dfecha}
GROUP BY
     correpo.codigo
ORDER BY
     correpo.cod_des ASC,
     correpo.fecha_des ASC]]>
	</queryString>
